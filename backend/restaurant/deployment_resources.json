    "policy": {
      "Version": "2012-10-17",
      "Statement": [
        {
          "Sid": "AllowPublicReadForAllObjects",
          "Effect": "Allow",
          "Principal": "*",
          "Action": "s3:GetObject",
          "Resource": "*"
        }
      ]
    },
    "resource_type": "s3_bucket",
    "LifecycleConfiguration": {
      "Rules": [
