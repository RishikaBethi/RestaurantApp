{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "SignUpRequest",
  "type": "object",
  "properties": {
    "firstName": {
      "type": "string",
      "minLength": 1,
      "maxLength": 50
    },
    "lastName": {
      "type": "string",
      "minLength": 1,
      "maxLength": 50
    },
    "email": {
      "type": "string",
      "format": "email"
    },
    "password": {
      "type": "string",
      "minLength": 8,
      "maxLength": 16
    }
  },
  "required": ["firstName", "lastName", "email", "password"],
  "additionalProperties": false
}
